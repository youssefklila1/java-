<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="747.0" prefWidth="1000.0" style="-fx-background-color: ffffff;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="middlefeastdes.gui.back.ArticleController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="1000.0" spacing="100.0">
         <children>
            <Button fx:id="viewCoursesBtn" mnemonicParsing="false" onAction="#loadViewCourses" styleClass="styledBtn" stylesheets="@../../css/style.css" text="View">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="addCourseBtn" layoutX="10.0" layoutY="40.0" mnemonicParsing="false" onAction="#loadAddCourse" styleClass="styledBtn" stylesheets="@../../css/style.css" text="Add">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="statsBtn" layoutX="260.0" layoutY="40.0" mnemonicParsing="false" onAction="#loadStats" styleClass="styledBtn" stylesheets="@../../css/style.css" text="Stats">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <AnchorPane fx:id="productDisplayArea" layoutY="100.0" prefHeight="647.0" prefWidth="1000.0" style="-fx-background-color: ffffff;" />
   </children>
</AnchorPane>
