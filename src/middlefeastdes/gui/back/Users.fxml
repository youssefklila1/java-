   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="1000.0" spacing="100.0">
         <children>
            <Button fx:id="viewUsersBtn" mnemonicParsing="false" onAction="#loadViewUsers" styleClass="styledBtn" stylesheets="@../../css/style.css" text="View">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="addUserBtn" layoutX="10.0" layoutY="40.0" mnemonicParsing="false" onAction="#loadAddUser" styleClass="styledBtn" stylesheets="@../../css/style.css" text="Add">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <AnchorPane fx:id="productDisplayArea" layoutY="100.0" prefHeight="647.0" prefWidth="1000.0" />
   </children>
</AnchorPane>
