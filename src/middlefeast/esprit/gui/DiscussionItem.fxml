<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="thematiqueItem" prefHeight="100.0" prefWidth="700.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="middlefeast.esprit.gui.DiscussionItemController">
   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="id_discussion" prefHeight="0.0" prefWidth="0.0" visible="false" />
            <Label fx:id="lbl_discussion" alignment="CENTER" contentDisplay="CENTER" text="discussion titre" textFill="#e4606d">
               <font>
                  <Font name="Arial Bold Italic" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="btn_voircommentaire" mnemonicParsing="false" stylesheets="@style.css" text="Voir Commentaire">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
   <styleClass>
      <String fx:value="thematique-item" />
      <String fx:value="shadow" />
   </styleClass>
</AnchorPane>
