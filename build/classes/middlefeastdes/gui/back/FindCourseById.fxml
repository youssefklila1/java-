            <HBox spacing="10.0">
               <children>
                  <Label text="From :" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblDateDebut" layoutX="10.0" layoutY="10.0" prefHeight="27.0" text="Label" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" spacing="31.0">
               <children>
                  <Label text="To :" textFill="WHITE">
